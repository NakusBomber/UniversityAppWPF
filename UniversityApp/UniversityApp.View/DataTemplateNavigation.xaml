<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vms="clr-namespace:UniversityApp.ViewModel.ViewModels.Pages;assembly=UniversityApp.ViewModel"
                    xmlns:views="clr-namespace:UniversityApp.View.Pages">


    <DataTemplate DataType="{x:Type vms:ShowViewModel}">
        <views:ShowPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:CourseViewModel}">
        <views:CourseManagementPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:GroupViewModel}">
        <views:GroupManagementPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:StudentViewModel}">
        <views:StudentManagementPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:TeacherViewModel}">
        <views:TeacherManagementPage/>
    </DataTemplate>
</ResourceDictionary>