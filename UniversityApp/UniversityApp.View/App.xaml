<Application x:Class="UniversityApp.View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UniversityApp.View"
             xmlns:converters="clr-namespace:UniversityApp.ViewModel.Converters;assembly=UniversityApp.ViewModel"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>

            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="DataTemplateNavigation.xaml"/>

            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
